{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4.2",
        "dompdf/dompdf": "^3.0",
        "fakerphp/faker": "^1.24",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image-laravel": "^1.5",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^12.42",
        "laravel/reverb": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.17",
        "laravel/spark-stripe": "^6.0",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maize-tech/laravel-markable": "^2.3",
        "mpociot/vat-calculator": "^3.13",
        "php-flasher/flasher-laravel": "^2.1",
        "socialiteproviders/discord": "^4.2",
        "spatie/laravel-event-sourcing": "^7.11",
        "spatie/laravel-permission": "^6.12",
        "spatie/laravel-sitemap": "^7.3",
        "symfony/http-client": "^7.2",
        "symfony/postmark-mailer": "^7.2",
        "tightenco/ziggy": "^2.5",
        "wildbit/postmark-php": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "laravel/breeze": "^2.3",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "laravel/pail": "^1.2.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://spark.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
